.report-item {
    align-items: center;
    flex-direction: row;
}

.icon {
    width: 16px;
    height: 16px;
    margin: 2px 4px;
    flex-shrink: 0;
}

.warning {
    background-image: resource('Icons/d_console.warnicon.png');
}

.info {
    background-image: resource('Icons/d_console.infoicon.png');
}

.error {
    background-image: resource('Icons/d_console.erroricon.png');
}
